{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
           "src": "/api/(.*)",
            "dest": "index.js"
        }
    ],
    "headers": [
        { "source": "/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=0, must-revalidate"}] }
      ]

}